<h1>Links</h1>
<ul id='links'>
  <% @links.each do |link| %>
    <li>
    Title: <%= link.title %>
    URL:  <a href= '<%= link.url %>' /> <%= link.title %> </a>
  </li>
<% end %>
</ul>
<!-- <br>
<h2> Add link? </h2> -->

<form action='/newlink' method="get">
    <button type="submit">New link</button>
</form>
